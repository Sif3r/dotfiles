# This is Git's per-user configuration file.
[user]
	name = Aur√©lien Aoustin
	email = 62019680+Sif3r@users.noreply.github.com
[core]
	editor = nvim
	compression = 9
	whitespace = error
	preloadindex = true
[color]
	ui = true
[alias]
	st = status
	ci = commit
	co = checkout
	br = branch
	di = diff
	lg = log --graph --all --decorate
	po = push origin
[mergetool "fugitive"]
	cmd = nvim -f -c \"Gdiff\" \"$MERGED\"
[merge]
	tool = fugitive
[pull]
	rebase = true
[init]
	defaultBranch = main
